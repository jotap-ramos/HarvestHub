@model HarvestHub.Models.Fornecedor
@{
	ViewData["Title"] = "Create";
}
<h3>Cadastre um novo fornecedor</h3>

<form asp-action="Create" method="post">
</form>
<form asp-action="Create" method="post">
	<div class="form-group">
		<label for="Nome">Nome</label>
		<input type="text" class="form-control" asp-for="Nome" />
	</div>
	<div class="form-group">
		<label for="Categoria">CNPJ</label>
		<input type="text" class="form-control" asp-for="CNPJ" />
	</div>
	<div class="form-group">
		<label for="Categoria">Telefone</label>
		<input type="text" class="form-control" asp-for="Telefone" />
	</div>
	<div class="form-group">
		<label for="Categoria">Email</label>
		<input type="text" class="form-control" asp-for="Email" />
	</div>
	<button type="submit" class="btn btn-primary">Submit</button>
</form>